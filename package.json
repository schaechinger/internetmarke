{
  "name": "internetmarke",
  "version": "0.1.0",
  "description": "A node wrapper for the Internetmarke web service of the Deutsche Post.",
  "keywords": [
    "internetmarke",
    "stamp",
    "post",
    "letter",
    "1c4a",
    "deutsche-post"
  ],
  "main": "index.js",
  "repository": "https://github.com/schaechinger/internetmarke.git",
  "author": "Manuel Sch√§chinger <manuel@schaechinger.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schaechinger/internetmarke/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/_mocha $(find test -name '*.test.js')",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha $(find test -name '*.test.js') --report lcovonly -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "md5": "2.2.1",
    "moment": "2.20.1",
    "moment-timezone": "0.5.14",
    "soap": "0.23.0"
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "istanbul": "0.4.5",
    "jsdoc": "3.5.5",
    "mocha": "5.0.0",
    "mocha-lcov-reporter": "1.3.0",
    "mock-require": "3.0.1",
    "should": "13.2.1",
    "sinon": "4.2.2"
  }
}
